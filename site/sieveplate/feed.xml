---
layout: none
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom" >
	<generator uri="https://jekyllrb.com/" version="4.3.2">
		Jekyll
	</generator>
	<link href="{{ site.url }}{{ page.url }}" rel="self" type="application/atom+xml" />
	<link href="{{ site.url }}" rel="alternate" type="text/html" />
	<updated> <!-- publish date of latest post -->
		{{ site.posts.first.date | date_to_xmlschema }}
	</updated>
	<id>
		{{ site.url }}/sieveplate
	</id>
	<title type="html">
		SievePlate
	</title>
	{% for post in site.posts %}
	<entry>
		<title type="html">
			{{ post.title | xml_escape }}
		</title>
		<link href="http://localhost:4000/sieveplate/2023/06/srb2-icon-tutorial/" rel="alternate" type="text/html" title="{{ post.title | xml_escape}}" />
		<published>
			{{ post.date | date_to_xmlschema }}
		</published>
		<updated>
			{{ post.date | date_to_xmlschema }}
		</updated>
		<id>
			{{ site.url }}{{ post.id }}
		</id>
		<content type="html" xml:base="{{ site.url }}{{ post.id }}">
			<![CDATA[
			{{ post.content }}
			]]>
		</content>
		{% assign author = site.data.authors | find: "id", post.author %}
		<author>
			<name>
				{{ author.fullname }}
			</name>
		</author>
		{% for tag in post.tags %}<category term="{{ tag }}" />
		{% endfor %}
		<!--
		<summary type="html">
			<![CDATA[
			{{ post.excerpt | xml_escape }}
			]]>
		</summary>
		-->
	</entry>
	{% endfor %}
</feed>